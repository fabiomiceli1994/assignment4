k = "0.100000"
N_x = "16"
tau = "0.001000"

set terminal pdf
set output "HE_plot_N_".N_x."_tau_".tau."_k_".k."_Heun3.pdf"
set xlabel "x"
set ylabel "u(x,t)"
set title "Time evolution of solution for Heun3 scheme (N_x=".N_x.", {/Symbol t}=".tau.", {/Symbol k}=".k.")"
set yrange [0:1.01]


set key reverse Left outside
set grid

plot for [i=0:5] "HE_k_".k."_N_".N_x."_tau_".tau."_scheme_2_t_".i.".dat" using 1:2 with l title "n=".i
